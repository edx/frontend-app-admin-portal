{
  "name": "frontend-app-admin-portal",
  "version": "0.1.0",
  "description": "The frontend-app-admin-portal is a frontend that provides branded learning experiences.",
  "repository": "https://github.com/edx/frontend-app-admin-portal",
  "scripts": {
    "build": "NODE_ENV=production BABEL_ENV=production webpack --config=config/webpack.prod.config.js",
    "build:gh-pages": "NODE_ENV=development BABEL_ENV=development webpack --config=config/webpack.dev.config.js",
    "deploy:gh-pages": "npm run build:gh-pages && gh-pages -d dist",
    "is-es5": "es-check es5 ./dist/*.js",
    "lint": "eslint --ext .js --ext .jsx .",
    "precommit": "npm run lint",
    "prepublishOnly": "npm run build",
    "start": "NODE_ENV=development BABEL_ENV=development webpack-dev-server --config=config/webpack.dev.config.js --hot --inline --progress",
    "debug-test": "node --inspect-brk node_modules/.bin/jest --runInBand --coverage",
    "test": "jest --coverage --runInBand",
    "snapshot": "jest --updateSnapshot"
  },
  "license": "AGPL-3.0",
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "@babel/runtime-corejs3": "^7.9.2",
    "@edx/frontend-auth": "^5.3.6",
    "@edx/frontend-logging": "^2.1.0",
    "@edx/paragon": "^9.0.0",
    "@redux-beacon/segment": "^1.1.0",
    "axios": "^0.18.1",
    "axios-mock-adapter": "^1.17.0",
    "babel-eslint": "^10.1.0",
    "classnames": "^2.2.5",
    "core-js": "^3.6.4",
    "faker": "^4.1.0",
    "file-saver": "^1.3.8",
    "font-awesome": "^4.7.0",
    "history": "^4.10.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "query-string": "^5.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^5.1.2",
    "react-responsive": "^6.1.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-spinners": "^0.3.2",
    "react-textarea-autosize": "^7.1.2",
    "redux": "^3.7.2",
    "redux-beacon": "^2.1.0",
    "redux-devtools-extension": "^2.13.8",
    "redux-form": "^7.4.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "regenerator-runtime": "^0.13.5",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@intervolga/optimize-cssnano-plugin": "^1.0.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.1.0",
    "babel-test": "^0.2.3",
    "chunk-rename-webpack-plugin": "^1.1.1",
    "clean-webpack-plugin": "^0.1.19",
    "codecov": "^3.6.1",
    "css-loader": "^3.5.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "es-check": "^5.1.0",
    "eslint-config-edx": "^4.0.3",
    "file-loader": "^1.1.9",
    "gh-pages": "^1.1.0",
    "html-webpack-exclude-assets-plugin": "0.0.7",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-new-relic-plugin": "^1.1.0",
    "html-webpack-plugin": "^3.0.3",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.2.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.13.1",
    "react-dev-utils": "^5.0.0",
    "react-test-renderer": "^16.12.0",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "jest": {
    "globalSetup": "./src/jestGlobalSetup.js",
    "setupFiles": [
      "./src/setupTest.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/segment.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!(@edx/paragon)/).*/"
    ]
  }
}
